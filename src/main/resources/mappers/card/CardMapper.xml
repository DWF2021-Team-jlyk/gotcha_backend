<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gotcha.www.card.dao.CardDAO">

    <select id="selectCard"
            resultType="com.gotcha.www.card.vo.CardDTO"
            parameterType="int"
    >
        select *
        from GC_CARD
        WHERE card_id = #{card_id}
    </select>

    <select
            id="selectCards"
            resultType="com.gotcha.www.card.vo.CardDTO"
            parameterType="int"
    >
        select *
        from GC_CARD
        WHERE ws_id = #{ws_id}
    </select>

</mapper>